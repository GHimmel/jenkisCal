

pipeline {
    agent any
   
    stages {
        stage('Ejecutar en paralelo') {
            parallel {
                stage('build') {
                    agent any
                    steps {
                        sh 'mvn clean package'
                    }
                }
                stage('test') {
                    agent any
                    steps {
                        sh 'mvn test'
                    }
                }
              
            }
        }
    }
}
